{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "jsx": "preserve",
    "strict": true,
    "noEmit": true,
    "types": ["astro/client"]
  },
  "include": ["src", "astro.config.*", "eslint.config.*", ".astro/types.d.ts", "**/*"],
  "exclude": ["dist"]
}
